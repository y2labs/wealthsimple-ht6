  "scripts": {
    "start": "node ./build/main.js",
    "dev": "./node_modules/.bin/dotenv -e ./.env.local ./node_modules/.bin/backpack dev",
    "build": "NODE_ENV=production ./node_modules/.bin/dotenv -e ./.env ./node_modules/.bin/backpack build",
    "deploy:api:prod": "now --dotenv .env.production --public && now alias && now rm ws --safe -y",
