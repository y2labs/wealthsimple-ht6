    "deploy:prisma:prod": "prisma deploy -e ./.env.production",
    "script:seed-normal-items:dev": "dotenv -e ./.env.local node ./scripts/seed-normal-items",
    "script:seed-normal-items:prod": "dotenv -e ./.env.production node ./scripts/seed-normal-items",
    "script:remove-all-items:dev": "dotenv -e ./.env.local node ./scripts/remove-all-items",
    "script:remove-all-items:prod": "dotenv -e ./.env.production node ./scripts/remove-all-items"
  }
